{
  "id": "midi-item-showcase-community",
  "title": "Midi Item Showcase - Community",
  "description": "A collection of Midi Automations from the Posney's Foundry Automation Discord",
  "version": "#{VERSION}#",
  "compatibility": {
    "minimum": "11",
    "verified": "11",
    "maximum": "11"
  },
  "authors": [
    {
      "name": "TMinz",
      "discord": "TMinz",
      "url": "https://github.com/txm3278"
    }
  ],
  "relationships": {
    "systems": [
      {
        "id": "dnd5e",
        "type": "system",
        "manifest": "https://raw.githubusercontent.com/foundryvtt/dnd5e/master/system.json",
        "compatibility": {
          "minimum": "2.4.1",
          "verified": "3.0.3"
        }
      }
    ],
    "requires": [
      {
        "id": "midi-qol",
        "type": "module",
        "manifest": "https://gitlab.com/tposney/midi-qol/raw/v10/package/module.json",
        "compatibility": {
          "verified": "11.4.3",
          "minimum": "11.3.23"
        }
      }
    ]
  },
  "esmodules": ["scripts/module.js"],
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "languages/en.json"
    }
  ],
  "packs": [
    {
      "name": "misc-items",
      "label": "MISC Items",
      "path": "packs/misc-items",
      "type": "Item",
      "system": "dnd5e",
      "ownership": {
        "PLAYER": "OBSERVER",
        "ASSISTANT": "OWNER"
      }
    },
    {
